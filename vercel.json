{
  "buildCommand": "dotnet workload install wasm-tools && dotnet publish src/TodoApp.Browser/TodoApp.Browser.csproj -c Release",
  "outputDirectory": "src/TodoApp.Browser/bin/Release/net9.0-browser/publish/wwwroot",
  "framework": null,
  "installCommand": "echo 'No npm install needed'",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
